<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cdc.notifactiondemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.cdc.adapter.ListActivityArrayAdapter1">
            <intent-filter>
                <action android:name="ListActivityArrayAdapter1" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name="com.cdc.adapter.ListActivityArrayAdapter2">
            <intent-filter>
                <action android:name="ListActivityArrayAdapter2" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name="com.cdc.adapter.ListActivityArrayAdapter3">
            <intent-filter>
                <action android:name="ListActivityArrayAdapter3" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name="com.cdc.notifaction.NotifactionResultActivity"></activity>
    </application>

</manifest>